// EXPECT: PASS
// INTERP_ARGS: --sym %?x=42
// COMPILER_ARGS: --sym %?x=42
// SOLVER_ARGS: --path ^entry
struct @Inner { val: i32; }
struct @Outer { inn: @Inner; }
fun @main(): i32 {
  sym %?x: value i32 in [0, 100];
  let %o: @Outer = { {%?x} };
^entry:
  require %o.inn.val == 42, "deep solve";
  ret 0;
}
