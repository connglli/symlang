// SKIP: COMPILER
// SKIP: INTERPRETER
// EXPECT: PASS
// ARGS: --main @main --path '^entry'

// Solve for %?area in [50, 100] such that 10 * 8 == %?area (with truncation).
struct @Rect { w: i32; h: i32; }
fun @main() : i32 {
  sym %?area : value i32 in [50, 100];
  let mut %r: @Rect = {10, 0};
^entry:
  %r.h = %?area / %r.w;
  require %r.h == 8, "height must be 8";
  ret 0;
}
