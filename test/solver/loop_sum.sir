// EXPECT: PASS
// SOLVER_ARGS: --main @main --path '^entry,^loop,^loop,^loop,^exit'
fun @main() : i32 {
  sym %?result : value i32;
  let mut %acc: i32 = 0;
  let mut %i: i32 = 0;
  let %one: i32 = 1;
  let %five: i32 = 5;

^entry:
  br ^loop;

^loop:
  %acc = %acc + %five;
  %i = %i + %one;
  br %i < 3, ^loop, ^exit;

^exit:
  require %?result == %acc, "result must match final accumulator value";
  ret 0;
}
