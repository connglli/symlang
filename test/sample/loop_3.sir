// EXPECT: PASS
// SOLVER_ARGS: --sample 1000 --require-terminal --seed 42
fun @main() : i32 {
  sym %?target : value i32 in [1, 3];
  let mut %i: i32 = 0;
  let mut %j: i32 = 0;

^entry:
  br ^outer;

^outer:
  br %i < 2, ^inner_init, ^exit;

^inner_init:
  %j = 0;
  br ^inner;

^inner:
  br %j < 2, ^inner_body, ^outer_inc;

^inner_body:
  %j = %j + 1;
  br ^inner;

^outer_inc:
  %i = %i + 1;
  br ^outer;

^exit:
  require %?target == 2;
  ret %i;
}
