// EXPECT: PASS
// INTERP_ARGS: --sym %?n=5
// COMPILER_ARGS: --sym %?n=5
fun @main() : i32 {
  sym %?n : value i32;
  let mut %res: i32 = 1;
  let mut %i: i32 = 1;
  let %one: i32 = 1;
  let %expected: i32 = 120;

^entry:
  br ^loop;

^loop:
  br %i <= %?n, ^body, ^exit;

^body:
  %res = %res * %i;
  %i = %i + %one;
  br ^loop;

^exit:
  require %res == %expected, "5! == 120";
  ret %res;
}
