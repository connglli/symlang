// EXPECT: PASS
// SOLVER_ARGS: --path ^entry

struct @Vec3 {
    data: [3] i32;
    len: i32;
}

fun @main(): i32 {
    let mut %v: @Vec3 = { {10, 20, 30}, 3 };

^entry:
    require %v.len == 3, "len check";
    require %v.data[0] == 10, "data[0]";
    require %v.data[1] == 20, "data[1]";
    require %v.data[2] == 30, "data[2]";

    %v.data[1] = 99;
    require %v.data[1] == 99, "data[1] mod";

    ret 0;
}
